{
  "name": "agent-orchestrator",
  "version": "1.0.0",
  "description": "Agent Orchestrator - A multi-agent orchestration system",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/.next apps/*/dist",
    "setup": "node scripts/setup.js",
    "dev:daemon": "pnpm --filter agent-orchestrator-daemon run dev",
    "dev:web": "pnpm --filter web run dev",
    "dev:memory": "cd apps/memory-service && venv/bin/python -m src.main"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "turbo": "^2.3.0",
    "typescript": "^5.9.3"
  }
}
