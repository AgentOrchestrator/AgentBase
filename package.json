{
  "name": "agent-orchestrator",
  "version": "1.0.0",
  "description": "Agent Orchestrator - A multi-agent orchestration system",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "overrides": {
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "type": "module",
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/.next apps/*/dist",
    "dev:daemon": "npm run dev --workspace=agent-orchestrator-daemon",
    "dev:desktop": "npm run build --workspace=@agent-orchestrator/shared && npm run dev --workspace=desktop",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:dev": "docker compose up",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart",
    "docker:clean": "docker compose down -v"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "turbo": "^2.3.0",
    "typescript": "^5.9.3"
  }
}
