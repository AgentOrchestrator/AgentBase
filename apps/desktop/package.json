  "description": "AgentBase Desktop App",
  "license": "MIT",
  "author": "Hai Dang",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && npm run build:main && electron .",
    "build": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "dev": "concurrently \"npm run build:main\" \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:vite": "vite",
    "electron": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^39.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xterm": "^5.3.0",
    "@xterm/addon-fit": "^0.10.0"
  }
}
